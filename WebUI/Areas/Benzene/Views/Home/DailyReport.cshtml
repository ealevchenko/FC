@using WebUI.App_GlobalResources;
@{
    ViewBag.Title = @GlobalResource.home_title;
    ViewBag.section = @GlobalResource.home_daili_report_benzone;
}

@section styles {
    @Styles.Render("~/jquery-ui/css")
    @Styles.Render("~/DataTables/css")
    @Styles.Render("~/DateTime/css")
    <link rel="stylesheet" type="text/css" href="~/Content/View/daily_accounting_report.css">
}

@section scripts {
    @Scripts.Render("~/bundles/Ajax")
    @Scripts.Render("~/bundles/jquery-ui")
    @Scripts.Render("~/bundles/DataTables")
    @Scripts.Render("~/bundles/DateTime")
    <script src="~/Scripts/jquery.cookie.js" type="text/javascript"></script>
    <script src="~/Scripts/View/common.js" type="text/javascript"></script>
    <script src="~/Scripts/View/benzene_daily_report.js" type="text/javascript"></script>
    <script>
        $('body').addClass('img-header-global-benzene');
    </script>
}

<div id="report">
    <div id="tabs-reports">
        <ul>
            <li><a href="#tabs-report-1" id="link-tabs-report-1"></a></li>
            @*<li><a href="#tabs-report-2" id="link-tabs-report-2"></a></li>*@
        </ul>
        <div id="tabs-report-1">
            <div id="table-panel">
            </div>
            <table class='display compact cell-border row-border hover' id="table-daili-report" style="width:100%" cellpadding="0">
            </table>
        </div>
    </div>

</div>







